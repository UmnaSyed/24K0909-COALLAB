INCLUDE Irvine32.inc

.data
msg BYTE "Sum of 10, 20, and 30 = ", 0
result DWORD ?

.code
main PROC

    push 10
    push 20
    push 30

    pop eax              
    pop ebx              
    add eax, ebx         
    pop ecx              
    add eax, ecx          

    mov edx, OFFSET msg 
    call WriteString     

    call WriteDec        
    call Crlf            

    exit
main ENDP
END main
